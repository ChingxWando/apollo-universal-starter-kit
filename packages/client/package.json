{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">= 6.11.1 <= 10",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production zen build",
    "clean": "rimraf build .tmp",
    "tests": "cross-env NODE_ENV=test PORT=7070 zen test -t 10000 --include src/__tests__/entry.ts --full-trace \"src/**/*.spec.*\" \"../../modules/**/client-react/**/*.spec.*\"",
    "tests:watch": "cross-env NODE_ENV=test PORT=7070 zen test -t 10000 --include src/__tests__/entry.ts --full-trace --watch \"src/**/*.spec.*\" \"../../modules/**/client-react/**/*.spec.*\"",
    "test": "yarn tests && yarn lint",
    "eslint": "eslint --fix --ext js --ext jsx --ext json src",
    "tslint": "tslint --fix -p tsconfig.json -c ../../tslint.json",
    "lint": "yarn eslint && yarn tslint",
    "watch": "zen -v watch",
    "heroku-postbuild": "yarn build",
    "cli": "node ../../tools/cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sysgears/apollo-universal-starter-kit"
  },
  "bugs": {
    "url": "https://github.com/sysgears/apollo-universal-starter-kit/issues"
  },
  "homepage": "https://github.com/sysgears/apollo-universal-starter-kit",
  "keywords": [
    "apollo",
    "universal",
    "starter",
    "graphql",
    "react",
    "redux"
  ],
  "author": "SysGears INC",
  "license": "MIT",
  "dependencies": {
    "@gqlapp/authentication-client-react": "^0.1.0",
    "@gqlapp/chat-client-react": "^0.1.0",
    "@gqlapp/contact-client-react": "^0.1.0",
    "@gqlapp/core-client-react": "^0.1.0",
    "@gqlapp/counter-client-react": "^0.1.0",
    "@gqlapp/favicon-common": "^0.1.0",
    "@gqlapp/forms-client-react": "^0.1.0",
    "@gqlapp/i18n-client-react": "^0.1.0",
    "@gqlapp/i18n-common-react": "^0.1.0",
    "@gqlapp/look-client-react": "^0.1.0",
    "@gqlapp/look-client-react-native": "^0.1.0",
    "@gqlapp/module-client-react": "^0.1.0",
    "@gqlapp/module-client-react-native": "^0.1.0",
    "@gqlapp/page-not-found-client-react": "^0.1.0",
    "@gqlapp/pagination-client-react": "^0.1.0",
    "@gqlapp/payments-client-react": "^0.1.0",
    "@gqlapp/post-client-react": "^0.1.0",
    "@gqlapp/reports-client-react": "^0.1.0",
    "@gqlapp/router-client-react": "^0.1.0",
    "@gqlapp/upload-client-react": "^0.1.0",
    "@gqlapp/validation-common-react": "^0.1.0",
    "@gqlapp/user-client-react": "^0.1.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@gqlapp/core-common": "^0.1.0",
    "@gqlapp/testing-client-react": "^0.1.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.8.0",
    "@types/react-router-dom": "^4.2.7",
    "babel-plugin-styled-components": "^1.3.0"
  },
  "peerDependencies": {
    "autoprefixer": "*",
    "@larix/zen": "^0.1.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
